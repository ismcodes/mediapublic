include ../../shared/components/edit-buttons
= console.log(this.title)
div(model=cid, data-fields='*')
  a(href="url").h1= "title"
  p= url
  p.recording-description= description
  if false
    p.organization-id
      organizationName
  else
    select.organization-id
  end
